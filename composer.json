{
    "name":        "arcanedev/route-viewer",
    "description": "Route Viewer is a package that allows you to view all Laravel routes eloquently",
    "keywords":    ["arcanedev", "laravel", "route", "viewer"],
    "homepage":    "https://github.com/ARCANEDEV/RouteViewer",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=7.0",
        "arcanedev/support": "~4.2"
    },
    "require-dev": {
        "phpunit/phpcov":  "~4.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\RouteViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\RouteViewer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanedev\\RouteViewer\\RouteViewerServiceProvider"
            ],
            "aliases": {
                "RouteViewer": "Arcanedev\\RouteViewer\\Facades\\RouteViewer"
            }
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.5.0\""
    }
}
